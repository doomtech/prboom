all: libdcdoom.a

include $(KOS_BASE)/Makefile.rules

OBJS = i_system.o sqrt.o

KOS_CFLAGS += -I. -I../. -I../../. -DHAVE_CONFIG_H -DDREAMCAST --fast-math -nostdlib -O3

libdcdoom.a: $(OBJS)
	$(KOS_AR) rcs libdcdoom.a $(OBJS)

clean:
	-rm -f *.o *.klf libdcdoom.a
